<launch>
		<arg name="room_yaml" default="flightroom.yaml"/>
		<arg name="quad" default="quad2"/>
	<group ns="$(arg quad)">
		<rosparam command="load" file="$(find quad_safety)/launch/$(arg room_yaml)"/>
		<node name="safety" pkg="quad_safety" type="boundary_stripped.py" output="screen" />
	</group>
</launch>